{% extends 'base.html.twig' %}

{% block title %}Pesanan{% endblock %}

{% block body %}
    <h1>Pesanan</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pesanan.id }}</td>
            </tr>
            <tr>
                <th>Pemesan</th>
                <td>{{ pesanan.pemesan }}</td>
            </tr>
            <tr>
                <th>Jumlah</th>
                <td>{{ pesanan.jumlah }}</td>
            </tr>
            <tr>
                <th>Proses</th>
                <td>{{ pesanan.proses }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_pesanan_index') }}">back to list</a>

    <a href="{{ path('app_pesanan_edit', {'id': pesanan.id}) }}">edit</a>

    {{ include('pesanan/_delete_form.html.twig') }}
{% endblock %}
